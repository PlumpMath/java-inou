<project name="logical-expression" default="javacc" basedir=".">

  <target name="javacc">
    <javacc 
      target="LogicalExpression.jj"
      javacchome="${javacchome}"
      outputdirectory="."
      />
  </target>

  <target name="clean">
    <delete>
      <!-- javaccfiles.txt Generated by following command -->
      <!-- grep "Generated By:JavaCC:" *.java|sed -e "s/\(\w*\.java\).*$/\1/g" > javaccfiles.txt -->
      <fileset dir="." includesfile="javaccfiles.txt"/>
    </delete>
  </target>

</project>
